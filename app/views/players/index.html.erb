<% @players.each do |player| %>
  <b><%= player.name %></b>
  <%= link_to("Show #{player.name}", player_path(player)) %>
  <%= button_to('Delete Player', player_path(player), method: :delete, data: {confirm: 'Delete them? Are you sure?!'}) %>
  <%= link_to('Edit Player', edit_player_path(player)) %>
  <br>
<% end %>